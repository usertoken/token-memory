{
  "name": "token-memory",
  "version": "0.0.9",
  "description": "temporal and durable memory as a module https://ut.usertoken.com/memory",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:spec && npm run test:cat && npm run test:cover",
    "test:spec": "./node_modules/.bin/mocha --reporter spec",
    "test:cat": "mocha --reporter nyan",
    "test:cover": "./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "cover": "npm run coverity:build && npm run coverity:compact && npm run coverity:upload",
    "coverity:build": "./node_modules/coveralls/bin/coveralls.js --dir cov-int --no-command --fs-capture-search ./",
    "coverity:compact": "tar zcf token-memory.tgz cov-int",
    "coverity:upload": "./scripts/coverity-submit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usertoken/token-memory.git"
  },
  "engines": {
    "node": "8.11.4",
    "npm": "6.4.1"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "keywords": [
    "memory",
    "nodejs",
    "browser",
    "react-native",
    "react",
    "storage",
    "grapql",
    "blockchain",
    "usertoken"
  ],
  "author": "UserToken Admin <admin@usertoken.com> (https://www.usertoken.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usertoken/token-memory/issues"
  },
  "homepage": "https://github.com/usertoken/token-memory#readme",
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "istanbul": "0.4.5",
    "mocha": "5.2.0"
  },
  "dependencies": {
    "gun": "0.9.99997",
    "gun-unset": "1.0.1"
  }
}
